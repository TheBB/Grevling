{
    parameters: { retcode: [0, 1] },
    script: ({retcode}) => [
        {
            command: ["sh", "-c", "echo stdout ${retcode} && >&2 echo stderr ${retcode}"],
            name: "good",
            capture-output: true,
        },
        {
            command: ["sh", "-c", "echo stdout ${retcode} && >&2 echo stderr ${retcode} && exit\${retcode}"],
            name: "bad",
        },
    ],
    settings: {
        logdir: ({retcode}) => "out-${retcode}",
    },
}
